[Unit]
Description=Pothole Detection Service
After=network.target

[Service]
Type=simple
User=admin
WorkingDirectory=/home/admin/pothole_/IOT-pothole-detection/raspi
ExecStart=/usr/bin/python3 /home/admin/pothole_/IOT-pothole-detection/raspi/main2.py
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target
